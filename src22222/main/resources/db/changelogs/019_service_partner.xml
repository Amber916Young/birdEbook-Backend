<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
      http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.1.xsd">
    <changeSet author="meng" id="19">
        <createTable tableName="service_partner">
            <column name="id" type="BIGINT" autoIncrement="true">
                <constraints nullable="false" primaryKey="true" unique="true"/>
            </column>

            <column name="years_of_experience" type="BIGINT"/>

            <column name="service_partner_name" type="VARCHAR(200)">
                <constraints nullable="false"/>
            </column>
            <column name="service_partner_email" type="VARCHAR(50)"/>
            <column name="description" type="VARCHAR(300)" />

            <column name="contact_number" type="VARCHAR(50)">
                <constraints nullable="false"/>
            </column>

            <column name="service_partner_logo_url" type="VARCHAR(100)" />
            <column name="category" type="VARCHAR(50)" >
                <constraints nullable="false"/>
            </column>

            <column  name="address_line1"  type="VARCHAR(50)">
                <constraints  nullable="false"/>
            </column>
            <column  name="address_line2"  type="VARCHAR(50)"/>
            <column  name="address_line3"  type="VARCHAR(50)"/>
            <column name="create_time" type="datetime" defaultValueComputed="CURRENT_TIMESTAMP"/>

        </createTable>

    </changeSet>
</databaseChangeLog>